  },
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "deliveryMethod": {
      "type": "enumeration",
      "enum": [
        "pickup",
        "shipping"
      ]
    },
    "address": {
      "type": "string"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "mercado_pago",
        "cash"
      ]
    },
    "mercadoPagoId": {
      "type": "string"
    },
    "orderStatus": {
      "type": "enumeration",
